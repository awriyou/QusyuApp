{
  "expo": {
    "name": "Qusyu",
    "slug": "Qusyu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.awriyou.Qusyu",
      "icon": {
        "light": "./assets/ic/Light.png",
        "dark": "./assets/ic/Dark.png",
        "tinted": "./assets/ic/Tinted.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic/adaptive-icon.png",
        "backgroundColor": "#12B09F"
      },
      "package": "com.awriyou.Qusyu"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f3112ae7-69ba-46b8-b4b3-90dc62dc8382"
      }
    },
    "plugins": [
      "expo-sqlite",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#12B09F",
          "image": "./assets/ic/splash-icon-light.png",
          "dark": {
            "image": "./assets/ic/splash-icon-dark.png",
            "backgroundColor": "#12B09F"
          },
          "imageWidth": 200
        }
      ]
    ]
  }
}
